{
	"name": "@bgroup/media-manager",
	"version": "1.0.0",
	"title": "Media Manager plugin",
	"description": "",
	"license": "ISC",
	"layout": "main-layout",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"backend": "backend",
				"platform": "web",
				"ports": {
					"bundles": 6510
				}
			},
			{
				"name": "web-tsc",
				"platform": "web",
				"environment": "development",
				"backend": "backend",
				"ports": {
					"bundles": 6514
				},
				"ts": {
					"compiler": "tsc"
				}
			},
			{
				"name": "backend",
				"environment": "development",
				"platform": "backend",
				"ports": {
					"bundles": 6511,
					"http": 6512,
					"inspect": 6513
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/backend": "~0.1.4",
		"@beyond-js/events": "^0.0.6",
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/react-18-widgets": "~0.0.1",
		"@beyond-js/reactive": "^1.1.6",
		"socket.io-client": "^4.5.4"
	}
}
